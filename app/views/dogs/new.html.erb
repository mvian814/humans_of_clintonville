<%= form_for @dog do |form| %>
<br>
  <%= form.label :name %>
  <%= form.text_field :name %>
<br>
  <%= form.label :breed %>
  <%= form.select :breed, @breeds %>
<br>
  <%= form.label :weight %>
  <%= form.text_field :weight %>
<br>
  <%= form.label :age %>
  <%= form.text_field :age %>
<br>
  <%= form.label :human %>
  <%= form.select :human_id, @humans  %>
<br>

<%= form.submit "Bark!", class: 'btn' %>

<% end %>
